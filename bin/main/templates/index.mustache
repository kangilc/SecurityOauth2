{{>layout/header}}
   <h1>Oauth2와 Security | Mustache | Spring Boot | Test</h1>
   <div class="col-md-12">
       <!--로그인 기능 영역-->
       <div class="row">
           <div class="col-md-6">
               <p><a href="/posts/save" role="button" class="btn btn-primary">글 등록</a></p>
               <p>
               {{#userName}}
                   Logged in as : <span id="user">{{userName}}</span>
                   <a href="/logout" class="btn btn-info active" role="button">Logout</a>
               {{/userName}}
               {{^userName}}
                   <a href="/oauth2/authorization/google" class="btn btn-success active" role="button">Google Login</a>
               {{/userName}}
               </p>
           </div>
       </div>
   </div>
   <br>
   <!-- 목록 출력 영역 -->
{{>layout/footer}}